   "source": [
    "Recursion- when function calls its self until the base condition or stop condition is true"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 18,
   "metadata": {},
   "outputs": [],
   "source": [
    "\n",
    "def recur(n):\n",
    "    original = n**2\n",
    "    if n < original:\n",
    "        return\n",
    "    \n",
    "    return recur(n*2)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 19,
   "metadata": {},
   "outputs": [],
   "source": [
    "recur(8)"
   ]
